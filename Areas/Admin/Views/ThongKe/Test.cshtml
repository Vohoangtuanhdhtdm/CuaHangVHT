
@{
    ViewData["Title"] = "Test";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayout.cshtml";
}

<div id="chart"></div>



@section Scripts
{  
	 <script>
			var options = {
				chart: {
					type: 'bar',
					height: 350,
				},
				series: [{
					name: 'Số lượng',
					data: [10, 20, 30, 40, 50]
				}],
				xaxis: {
					categories: ['Tháng 1', 'Tháng 2', 'Tháng 3', 'Tháng 4', 'Tháng 5']
				}
			};

			var chart = new ApexCharts(document.querySelector("#chart"), options);
			chart.render();
	</script>

}